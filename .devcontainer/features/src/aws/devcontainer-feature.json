{
  "id": "aws",
  "version": "1.0.0",
  "name": "AWS CLI",
  "description": "Installs AWS CLI",
  "options": {
    "version": {
      "type": "string",
      "description": "Select or enter an AWS CLI version.",
      "proposals": ["latest"],
      "default": "latest"
    },
    "awsIamEmail": {
      "type": "string",
      "description": "Enter your AWS IAM email address.",
      "required": true
    }
  },
  "installsAfter": ["ghcr.io/devcontainers/features/common-utils"]
}
