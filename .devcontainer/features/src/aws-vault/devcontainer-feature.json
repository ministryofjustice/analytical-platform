{
  "id": "aws-vault",
  "version": "1.0.0",
  "name": "AWS Vault",
  "description": "Installs AWS Vault",
  "options": {
    "version": {
      "type": "string",
      "description": "Select or enter an AWS Vault version.",
      "proposals": ["latest"],
      "default": "latest"
    }
  },
  "mounts": [
    { "source": "ministryofjustice-data-platform-devcontainer-awsvault", "target": "/home/vscode/.awsvault", "type": "volume" }
  ],
  "installsAfter": ["./features/src/aws"]
}
